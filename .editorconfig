# top-most EditorConfig file
root = false

# Unix-style newlines with a newline ending every file
[*]
indent_style = space
indent_size = 2
end_of_line = lf
insert_final_newline = true

[*.{js,ts,tsx}]
charset = utf-8
indent_style = space
indent_size = 2


[*.tsx]
charset = utf-8
indent_style = space
indent_size = 2
